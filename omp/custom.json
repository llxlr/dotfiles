{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "background": "transparent",
          "foreground": "#1865f5",
          "properties": {
            "linux": "\ue712",
            "macos": "\ue711",
            "ubuntu": "\ue73a",
            "windows": "\ue62a",
            "display_distro_name": true
          },
          "style": "diamond",
          "template": "{{ if .WSL }}\ue712 {{ end }}{{ .Icon }} ",
          "type": "os"
        },
        {
          "background": "transparent",
          "foreground": "#e06c75",
          "style": "plain",
          "template": "\uF0E7",
          "type": "root"
        },
        {
          "background": "transparent",
          "foreground": "#100e23",
          "style": "diamond",
          "template": "{{ if .SSHSession }}\uF817 {{ end }}{{ if .Root }}<#801dae>@</><#1865f5>{{ .HostName }}</>{{ else }}<#45f1c2>{{ .UserName }}</><#801dae>@</><#1865f5>{{ .HostName }}</>{{ end }} ",
          "type": "session"
        },
        {
          "background": "transparent",
          "foreground": "#55B9c4",
          "properties": {
            "folder_separator_icon": "/",
            "style": "full",
            "mapped_locations": {
              "C:/Users/i/AppData/Local/Temp": "\uf114",
              "D:/Documents/GitHub": "\ue708",
              "D:/Documents/PyCharmProjects": "\ue235",
              "D:/Documents/R": "\ufcd2",
              "D:/Documents/www": "\ufa9e",
              "D:/Documents/gopath/src": "\ue724",
              "D:/OneDrive": "\ue762",
              "D:/Games": "\ue25f",
              "D:/OneDrive/Markdown": "\ue73e"
            }
          },
          "style": "plain",
          "template": "\uf07b {{ .Path }} ",
          "type": "path"
        },
        {
          "background": "transparent",
          "foreground": "blue",
          "style": "plain",
          "template": "{{ if .Error }}{{ .Error }}{{ else }}{{ if .Version }}\uf487 {{ if .Name }}{{ .Name }}{{ end }} {{.Version}}{{ end }}{{ end }} ",
          "type": "project"
        },
        {
          "background": "transparent",
          "foreground": "#000000",
          "style": "plain",
          "template": " \uFD03 {{ .EnvironmentName }} ",
          "type": "az"
        },
        {
          "background": "transparent",
          "foreground": "#ffffff",
          "style": "plain",
          "template": " \uE7AD {{ .Profile }}{{if .Region }}@{{ .Region }}{{ end }} ",
          "type": "aws"
        },
        {
          "background": "transparent",
          "style": "plain",
          "template": " \uE77F {{ .Full }} ",
          "type": "dotnet"
        },
        {
          "background": "transparent",
          "foreground": "#c678dd",
          "properties": {
            "fetch_status": true
          },
          "style": "plain",
          "template": "<#ffffff>on</> {{ .HEAD }}{{ .BranchStatus }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }} ",
          "type": "git"
        },
        {
          "background": "transparent",
          "foreground": "#98c379",
          "properties": {
            "fetch_version": true
          },
          "style": "plain",
          "template": "<#ffffff>via</> \ue781 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} ",
          "type": "node"
        },
        {
          "background": "transparent",
          "foreground": "#ffe873",
          "properties": {
            "fetch_version": true,
            "fetch_virtual_env": true,
            "display_default": true,
            "display_mode": "context"
          },
          "style": "plain",
          "template": "<#ffffff>via</> üêç {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ if .Venv }} <#906cff>({{ .Venv }})</>{{ end }}{{ end }} ",
          "type": "python"
        },
        {
          "background": "transparent",
          "foreground": "#7fd5ea",
          "style": "plain",
          "template": "<#ffffff>via</> \uFCD1 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "type": "go"
        },
        {
          "background": "transparent",
          "foreground": "blue",
          "style": "plain",
          "template": "<#ffffff>via</> \ufcd2 {{ .Full }} ",
          "type": "r"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "background": "transparent",
          "foreground": "white",
          "properties": {
            "always_enabled": true
          },
          "style": "plain",
          "template": "\ueb58 {{ .FormattedMs }} ",
          "type": "executiontime"
        },
        {
          "background": "transparent",
          "foreground": "#23d18b",
          "properties": {
            "always_enabled": true
          },
          "style": "diamond",
          "template": "{{ if gt .Code 0 }}<#ff0000>\uf00d</>{{ else }}\uf42e{{ end }} ",
          "type": "exit"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "background": "transparent",
          "foreground": "#63f08c",
          "style": "plain",
          "template": "{{ if .Root }}<#e06c75>#</>{{ else }}${{ end }}",
          "type": "text"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "newline": true,
      "segments": [
        {
          "background": "transparent",
          "foreground": "#ffffff",
          "properties": {
            "url": "https://wakatime.com/api/v1/users/current/summaries?start=today&end=today&api_key={{ .Env.WAKATIME_API_KEY }}",
            "cache_timeout": 10,
            "http_timeout": 500
          },
          "style": "plain",
          "template": "{{ secondsRound .CumulativeTotal.Seconds }}",
          "type": "wakatime"
        }
      ]
    }
  ],
  "transient_prompt": {
    "background": "transparent",
    "foreground": "#63f08c",
    "template": "$ "
  },
  "valid_line": {
    "background": "transparent",
    "foreground": "#ffffff",
    "template": "<#e0def4,#286983>\uf42e </><#286983,transparent>\ue0b4</> "
  },
  "error_line": {
    "background": "transparent",
    "foreground": "#ffffff",
    "template": "<#eb6f92,#286983>\ue009 </><#286983,transparent>\ue0b4</> "
  },
  "tooltips": [
    {
      "background": "#fffb38",
      "foreground": "#193549",
      "leading_diamond": "ÓÇ∂",
      "trailing_diamond": "ÓÇ¥",
      "template": "{{ .HEAD }}{{ if .Staging.Changed }} \uF046 {{ .Staging.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Working.Changed }} \uF044 {{ .Working.String }}{{ end }}",
      "properties": {
        "fetch_status": true,
        "fetch_upstream_icon": true
      },
      "style": "diamond",
      "tips": ["git", "g"],
      "type": "git"
    }
  ],
  "console_title_template": "{{ .Folder }}{{if .Root}} :: Admin{{end}} :: {{.Shell}}",
  "final_space": true,
  "version": 2
}
